<keymap version="1" name="Default copy" parent="$default">
  <action id="ActivateLogcatToolWindow">
    <keyboard-shortcut first-keystroke="alt 6" />
  </action>
  <action id="ActivateTODOToolWindow" />
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="ctrl alt left" second-keystroke="ctrl alt back_space" />
  </action>
  <action id="JumpToLastChange">
    <keyboard-shortcut first-keystroke="shift ctrl back_space" />
    <keyboard-shortcut first-keystroke="ctrl alt back_space" />
  </action>
  <action id="JumpToNextChange">
    <keyboard-shortcut first-keystroke="shift ctrl alt back_space" />
  </action>
  <action id="NewElementInMenuGroup">
    <keyboard-shortcut first-keystroke="alt n" />
  </action>
  <action id="TogglePresentationMode">
    <keyboard-shortcut first-keystroke="ctrl semicolon" />
  </action>
</keymap>